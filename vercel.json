{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/Home",
      "destination": "/index.html"
    },
    {
      "source": "/Login",
      "destination": "/Authenticate.html"
    },
    {
      "source": "/Create",
      "destination": "/Authenticate1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ]
}

