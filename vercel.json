{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Home",
      "dest": "./index.html"
    },
    {
      "src": "/Login",
      "dest": "./Authenticate.html"
    },
    {
      "src": "/Create",
      "dest": "./Authenticate1.html"
    },
    
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ],
  "cleanUrls": true
}
